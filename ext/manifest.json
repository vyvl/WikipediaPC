{
    "applications": {
        "gecko": {
            "id": "nomobilewiki@vyvl.github.com"
        }
    },
  "name": "Wiki no mobile",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "No more mobile wikipedia.",
  "homepage_url": "http://github.com/vyvl",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "*://*.wikipedia.org/*",
    "webNavigation",
    "tabs"
  ]
}